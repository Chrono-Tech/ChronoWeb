header.header
	.wrap
		.header__logo
			+svg('header__logo-i', 'logo-chronobank')
			| #[span.footer__logo-color Chrono]bank.io
		.nav
			img(src="../i/menu.png" srcset="../i/@2x/menu.png 2x").nav__i
			.nav__block
				img(src="../i/logo.png" srcset="../i/@2x/logo.png 2x").nav__logo
				.nav__close.js-close-menu
				ul.nav__list
					li.nav__item.nav__item_hover.nav__item_expand
						a(href="#").nav__item.nav__item_label Products
						ul.nav__products
							li
								a(href="#")
									img(src="../i/nav_logo1.png" srcset="../i/@2x/nav_logo1.png 2x").nav__products-logo
									.nav__products-name ChronoMint
									.nav__products-descr Our product helps people lorem ipsum subtitle
							li
								a(href="#")
									img(src="../i/nav_logo2.png" srcset="../i/@2x/nav_logo2.png 2x").nav__products-logo
									.nav__products-name LaborX
									.nav__products-descr Our product helps people lorem ipsum subtitle
					li.nav__item.nav__item_hover
						a(href="/team.html").nav__item.nav__item_label.js-close-menu Team
					li.nav__item.nav__item_hover
						a(href="/faq.html").nav__item.nav__item_label.js-close-menu FAQ
					li.nav__item.nav__item_hover
						a(href="https://medium.com/@Chronobank" target="_blank").nav__item.nav__item_label.js-close-menu Blog
					li.nav__item.nav__item_hover
						a(href="#").nav__item.nav__item_label.js-close-menu Contacts
					li.nav__item.nav__item_hover
						a(href="#").nav__item.nav__login.nav__item_label.js-close-menu
							+svg('nav__login-i', 'login')
							| Login
					li.nav__lang
						a(href="#").nav__lang-current.js-nav-sec Eng
						ul.nav__lang-list
							li
								a(href="#").current
									+svg('nav__lang-i', 'menu')
									| Rus
							li
								a(href="#")
									+svg('nav__lang-i', 'menu')
									| Rus
							li
								a(href="#")
									+svg('nav__lang-i', 'menu')
									| Rus
								a(href="#")
									+svg('nav__lang-i', 'menu')
									| Rus
							li
								a(href="#")
									+svg('nav__lang-i', 'menu')
									| Rus
							li
								a(href="#")
									+svg('nav__lang-i', 'menu')
									| Rus
